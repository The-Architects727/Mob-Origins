{
    "name": "Health",
    "description": "You have 300 health. You gain immunity to projectiles when below half health. You regen 1 health every second, but cannot eat.",
    "type": "origins:multiple",
    "undead": {
      "type": "origins:entity_group",
      "group": "undead"
    },
    "effects": {
      "type": "origins:effect_immunity",
      "effects": [
        "minecraft:slowness",
        "minecraft:haste",
        "minecraft:mining_fatigue",
        "minecraft:jump_boost",
        "minecraft:nausea",
        "minecraft:regeneration",
        "minecraft:resistance",
        "minecraft:fire_resistance",
        "minecraft:water_breathing",
        "minecraft:invisibility",
        "minecraft:blindness",
        "minecraft:night_vision",
        "minecraft:hunger",
        "minecraft:weakness",
        "minecraft:poison",
        "minecraft:wither",
        "minecraft:health_boost",
        "minecraft:absorption",
        "minecraft:saturation",
        "minecraft:luck",
        "minecraft:slow_falling",
        "minecraft:conduit_power"
      ]
    },
    "regen": {
      "type": "origins:action_over_time",
      "interval": 20,
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_resource",
        "comparison": "<",
        "compare_to": 300
      },
      "entity_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:change_resource",
            "resource": "*:*_resource",
            "operation": "add",
            "change": 1
          },
          {
            "type": "origins:feed",
            "food": 2,
            "saturation": 2
          }
        ]
      }
    },
    "nofood": {
      "type": "origins:prevent_item_use",
      "item_condition": {
        "type": "origins:food"
      }
    },
    "resource": {
      "type": "origins:resource",
      "min": 0,
      "max": 300,
      "start_value": 300,
      "hud_render": {
        "should_render": true,
        "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
        "bar_index": 9
      }
    },
    "armor": {
      "type": "origins:invulnerability",
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_resource",
        "comparison": "<=",
        "compare_to": 150
      },
      "damage_condition": {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:projectile",
              "projectile": "minecraft:arrow"
            },
            {
              "type": "origins:projectile",
              "projectile": "minecraft:spectral_arrow"
            },
            {
              "type": "origins:projectile",
              "projectile": "minecraft:trident"
            },
            {
              "type": "origins:fire"
            }
          ]
      }
    },
    "immunities": {
      "type": "origins:invulnerability",
      "damage_condition": {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:fire"
            },
            {
              "type": "origins:name",
              "name": "drown"
            },
            {
              "type": "origins:name",
              "name": "freeze"
            }
          ]
      }
    },
    "breakdamage": {
      "type": "origins:action_when_damage_taken",
      "cooldown": 40,
      "entity_action": {
        "type": "origins:execute_command",
        "command": "fill ~1 ~-1 ~1 ~-1 ~2 ~-1 air destroy"
      }
    },
    "death": {
      "type": "origins:prevent_death",
      "entity_action": {
        "type": "origins:heal",
        "amount": 20
      },
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_resource",
        "comparison": ">=",
        "compare_to": 1
      }
    },
    "death2": {
      "type": "origins:action_over_time",
      "interval": 20,
      "entity_action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:damage",
            "amount": 100,
            "source": {
              "bypasses_armor": true,
              "unblockable": true,
              "name": "player"
            }
          },
          {
            "type": "origins:delay",
            "ticks": 20,
            "action": {
              "type": "origins:change_resource",
              "resource": "*:*_resource",
              "operation": "set",
              "change": 300
            }
          }
        ]
        
      },
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_resource",
        "comparison": "==",
        "compare_to": 0
      }
    },
    "respawn": {
      "type": "origins:action_on_callback",
      "entity_action_respawned": {
        "type": "origins:change_resource",
              "resource": "*:*_resource",
              "operation": "set",
              "change": 300
      }
    },
