{
    "type": "origins:multiple",
    "hidden": true,
    "friendermen_make_friends": {
        "type": "origins:action_over_time",
        "interval": 10,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "team join shulker @e[type=enderman]"
                },
                {
                    "type": "origins:execute_command",
                    "command": "team join shulker @e[type=endermite]"
                },
                {
                    "type": "origins:execute_command",
                    "command": "team join shulker @e[type=shulker]"
                },
                {
                    "type": "origins:execute_command",
                    "command": "team join shulker @e[type=shulker_bullet]"
                }
            ]
        }
    }
}
