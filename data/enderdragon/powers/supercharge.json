{
    "name": "Supercharged Charge",
    "description": "You can press your [Primary] to enter your charged mode. While in this state you can phase through obsidian and bedrock and will break all other blocks. ",
    "type": "origins:multiple",
    "bar": {
        "hidden": true,
        "type": "origins:resource",
        "min": 0,
        "max": 150,
        "hud_render": {
            "should_render": true,
            "bar_index": 2
        }
    },
    "boolean": {
        "hidden": true,
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "hud_render": {
            "should_render": false
        }
    },
    "activate": {
        "hidden": true,
        "type": "origins:active_self",
        "key": {
            "key": "key.origins.primary_active"
        },
        "cooldown": 400,
        "entity_action": {
            "type": "origins:change_resource",
            "resource":"*:*_boolean",
            "operation": "set",
            "change": 1
        },
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 6
        }
    },
    "deactivate": {
        "hidden": true,
        "type": "origins:action_over_time",
        "interval": 5,
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_bar",
            "comparison": "<=",
            "compare_to": 0
        },
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource":"*:*_boolean",
                    "operation": "set",
                    "change": 0
                },
                {
                    "type": "origins:change_resource",
                    "resource":"*:*_bar",
                    "operation": "set",
                    "change": 150
                }
            ]
        }
    },
    "drain": {
        "hidden": true,
        "type": "origins:action_over_time",
        "interval": 1,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_boolean",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_bar",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        },
        "entity_action": {
            "type": "origins:change_resource",
            "resource":"*:*_bar",
            "operation": "add",
            "change": -1
        }
    },
    "break": {
        "hidden": true,
        "type": "origins:action_over_time",
        "interval": 2,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_boolean",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_bar",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        },
        "entity_action": {
            "type": "origins:execute_command",
            "command": "fill ~-9 ~-1 ~-9 ~9 ~5 ~9 air replace #enderdragon:destroyable"
        }
    },
    "damage": {
        "hidden": true,
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:area_of_effect",
            "include_target": false,
            "radius": 10,
            "bientity_action": {
                "type": "origins:target_action",
                "action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:damage",
                            "amount": 5,
                            "source": {
                                "name": "player"
                            }
                        },
                        {
                            "type": "origins:chance",
                            "chance": 0.5,
                            "action": {
                                "type": "origins:damage",
                                "amount": 5,
                                "source": {
                                    "name": "player"
                                }
                            }
                        },
                        {
                            "type": "origins:chance",
                            "chance": 0.1,
                            "action": {
                                "type": "origins:damage",
                                "amount": 3,
                                "source": {
                                    "name": "player"
                                }
                            }
                        },
                        {
                            "type": "origins:add_velocity",
                            "space": "local",
                            "y": 2
                        }
                    ]

                }
            }
        }
    },
    "phase": {
        "type": "origins:phasing",
        "render_type": "blindness",
        "view_distance": 10,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:resource",
                    "resource": "*:*_boolean",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_bar",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        }

    },
    "speed": {
        "type": "origins:modify_air_speed",
        "modifier": {
            "operation": "multiply_total",
            "value": 0.5
        }
    }
    
}